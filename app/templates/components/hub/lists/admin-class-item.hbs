<tr>
  <td>{{@model.organization}}</td>
  <td>{{@model.classInformation.name}}</td>
  <td>{{@model.instructor.displayName}}</td>
  <td>{{@model.location.name}}</td>
  <td>
    {{#each @model.times as |t|}}
      {{time t.start 'h:mma'}}-{{time t.end 'h:mma'}} {{time t.start 'M/D/YY'}}
    {{/each}}
  </td>
  <td>
    <a href="#" class="text-danger" {{on "click" (prevent-default (fn @onDelete @model))}}>
      <i class="fa fa-fw fa-trash"></i>
    </a>
  </td>
</tr>
